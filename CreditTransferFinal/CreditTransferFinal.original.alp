			<Text><![CDATA[/**
 * PointMap
 */	
public class PointMap implements java.io.Serializable {

	/**
	 * This number is here for model snapshot storing purpose<br>
	 * It needs to be changed when this class gets changed
	 */ 
	private static final long serialVersionUID = 1L;
	
	
	HashMap pointMap;
    /**
     * Default constructor
     */
    public PointMap(int size){
    	
    	this.pointMap = new HashMap<Integer, PointEntry>(size);
    	for (int i = 0; i < size; i++)
    		this.pointMap.put(i, new PointEntry());
    	
    }
    
public PointMap(int size, int size2){
    	
    	this.pointMap = new HashMap<Integer, PointEntry>(size);
    	for (int i = 0; i < size; i++)
    		this.pointMap.put(i, new PointEntry(size2));
    	
    }
  
	public void addEntry (int key, double tValue){
		
		PointEntry entry = new PointEntry();
		entry.AddValue(tValue);
		this.pointMap.put(key, entry);
		
	}
	
	public void updateEntry (int key, double tValue){
		
		PointEntry entry = (PointEntry) this.pointMap.get(key);
		entry.AddValue(tValue);
		this.pointMap.put(key, entry);
		
	}
	
public void updateIndividualEntry (int key, int index, double tValue){
		
		PointEntry entry = (PointEntry) this.pointMap.get(key);
		double temp = entry.getValue(index);
		tValue = tValue + temp;
		entry.setValue(index, tValue);
		this.pointMap.put(key, entry);
		
	}

public void substractIndividualEntry (int key, int index, double tValue){
	
	PointEntry entry = (PointEntry) this.pointMap.get(key);
	double temp = entry.getValue(index);
	temp = temp - tValue;
	entry.setValue(index, temp);
	this.pointMap.put(key, entry);
	
}

public void setFriend (int key, double isFriend){
	
	PointEntry entry = (PointEntry) this.pointMap.get(key);
	entry.setValue(3, isFriend);
	this.pointMap.put(key, entry);
	
}

public boolean isFriend (int key){
	
	PointEntry entry = (PointEntry) this.pointMap.get(key);
	return entry.checkInteraction();
	
}
	
	public PointEntry getEntry (int key){
		
		PointEntry entry = (PointEntry) this.pointMap.get(key);
		return entry;
		
	}
	
	public double getSum (int key){
		
		double sum = 0.0;
		PointEntry entry = (PointEntry) this.pointMap.get(key);
		sum = entry.getSum();
		return sum;
		
	}
	
public double getAvg (int key){
		
		double avg = 0.0;
		PointEntry entry = (PointEntry) this.pointMap.get(key);
		avg = entry.getAvg();
		return avg;
		
	}

public String printEntry (int key){
	
	PointEntry entry = (PointEntry) this.pointMap.get(key);
	return entry.PrintEntry();
	
}

public void printCreditMap (){
	
	for (int i = 0; i < this.pointMap.size(); i++){
	PointEntry entry = (PointEntry) this.pointMap.get(i);
	System.out.println("Entry for agent " + i + " is: " + entry.PrintEntry());
	}
}

}]]></Text>
