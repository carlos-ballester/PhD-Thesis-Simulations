double minutes_connected = normal(5, 200, 30, 25);
connection_time = minutes_connected;
double cost = 0.0;
boolean abletoconnect = false;

if (get_Main().agents.get(id).dispositionaltrust == 100)
	cost = 0.0;
else
	cost = (1 - (double)(get_Main().agents.get(id).dispositionaltrust/100)) * minutes_connected;

if (balance >= cost){
	abletoconnect = true;
	//update entries
	this.pointMap.updateIndividualEntry(id,0,(minutes_connected/2));
	get_Main().agents.get(id).dataAllowance = get_Main().agents.get(id).dataAllowance - (connection_time*normal(0.2, 1, 0.75, 25));
	if(isMalicious)
		get_Main().agents.get(id).pointMap.updateIndividualEntry(this.getIndex(),2,1);
	else
		get_Main().agents.get(id).pointMap.updateIndividualEntry(this.getIndex(),0,1);
	return abletoconnect;
}else{
	
	ArrayList<Integer> lenders = searchForLenders(id);
	if (!lenders.isEmpty()){

		for(int i = 0; i < lenders.size(); i++){
			
			double balance_temp_subject = computeOwnBalance(lenders.get(i));
			double balance_temp_recommender = get_Main().agents.get(lenders.get(i)).computeOwnBalance(id);
			if (balance_temp_subject > cost && balance_temp_recommender > (2*cost)){
				
				abletoconnect = true;
				//update entries
				this.pointMap.updateIndividualEntry(id,0,(minutes_connected/2));
				get_Main().agents.get(id).dataAllowance = get_Main().agents.get(id).dataAllowance - (connection_time*normal(0.2, 1.5, 0.75, 25));
				if(isMalicious)
					get_Main().agents.get(id).pointMap.updateIndividualEntry(this.getIndex(),2,1);
				else
					get_Main().agents.get(id).pointMap.updateIndividualEntry(this.getIndex(),0,1);
					
				get_Main().agents.get(id).pointMap.substractIndividualEntry(lenders.get(i),0,cost);	
				get_Main().agents.get(lenders.get(i)).pointMap.substractIndividualEntry(this.getIndex(),0,cost);	
				return abletoconnect;
				
				}
			}
		}
	}

return abletoconnect;]]></Body>
