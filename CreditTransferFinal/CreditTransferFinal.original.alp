			<Functions>
				<Function AccessType="default" StaticFunction="false">
					<ReturnType><![CDATA[void]]></ReturnType>
					<Id>1383139510140</Id>
					<Name><![CDATA[createFriendships]]></Name>
					<X>-190</X><Y>120</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Body><![CDATA[isinit = false;
System.out.println("Started friend assignment");

//coworker assignment
for (int i = 0; i < agents.size(); i++){
if (agents.get(i).isLocalworker){

for (int j = 0; j < agents.size(); j++) {
if (i != j && agents.get(j).isLocalworker){

agents.get(i).pointMap.setFriend(j, 1.0);
agents.get(j).pointMap.setFriend(i, 1.0);

}

}

}
}

for (int i = 0; i < agents.size(); i++){

int friendnum = agents.get(i).friendamount;


if (agents.get(i).isForeigner){
	for (int j = 0; j < (friendsperforeignperson - friendnum); j++){
	System.out.println("Selecting random friend for foreigner");
	Double temp = uniform(0, agents.size()-1);
	int randfriend = temp.intValue();
	if (agents.get(i).pointMap.isFriend(randfriend) || randfriend == i || (agents.get(randfriend).isForeigner && agents.get(randfriend).friendamount >= friendsperforeignperson)){
	j--;
	}else{
	agents.get(i).pointMap.setFriend(randfriend, 1.0);
	agents.get(i).friendamount++;
	agents.get(randfriend).pointMap.setFriend(i, 1.0);
	agents.get(randfriend).friendamount++;
	}
	}
	}else{
	
	for (int j = 0; j < (friendsperlocalperson - friendnum); j++){
	System.out.println("Selecting random friend for local");
	Double temp = uniform(0, agents.size()-1);
	int randfriend = temp.intValue();
	if (agents.get(i).pointMap.isFriend(randfriend) || randfriend == i || (agents.get(randfriend).isForeigner && agents.get(randfriend).friendamount >= friendsperforeignperson)){
	j--;
	}else{
	agents.get(i).pointMap.setFriend(randfriend, 1.0);
	agents.get(i).friendamount++;
	agents.get(randfriend).pointMap.setFriend(i, 1.0);
	agents.get(randfriend).friendamount++;
	}
	}
	}
}
System.out.println("Finished friend assignment");]]></Body>
