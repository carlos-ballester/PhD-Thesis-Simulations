				<Function AccessType="default" StaticFunction="false">
					<ReturnType><![CDATA[void]]></ReturnType>
					<Id>1379947243968</Id>
					<Name><![CDATA[tryConnect]]></Name>
					<X>20</X><Y>360</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Body><![CDATA[ArrayList<Integer> hotspots = inRange();
double distance = 40.0;
double distancetemp = 0.0;
int inter_best_hotspot = -1;
int best_hotspot = -1;

if (!hotspots.contains(this.lastAP) || hotspots.size() > 1){

for (int i = 0; i < hotspots.size(); i++)
	{

		distancetemp = 0.0;
		distancetemp = distanceTo(get_Main().agents.get(hotspots.get(i)));

		if (distancetemp < distance  && hotspots.get(i) != this.lastAP) {

			distance = distancetemp;
			inter_best_hotspot = hotspots.get(i);

		}
	}
	
	if (inter_best_hotspot >= 0 && pointTransfer(inter_best_hotspot))
	best_hotspot = inter_best_hotspot;
	
	}
	
	if (lastAP != best_hotspot && lastAP >= 0){
	
	get_Main().agents.get(lastAP).hasConnections = false;
	this.lastAP = best_hotspot;
	this.isConected = false;
	this.setVelocity(1/second());
	disconnectFromAll();
	
	}else{
	
	this.lastAP = best_hotspot;
	this.isConected = false;
	this.setVelocity(1/second());
	disconnectFromAll();
	
	}
	
	if (best_hotspot >= 0){
	
	disconnectFromAll();
	this.setVelocity(0.2/second());
	this.isConected = true;
	get_Main().agents.get(best_hotspot).hasConnections = true;
	this.lastAP = best_hotspot;
	connectTo(get_Main().agents.get(best_hotspot));
	if(!this.ever_connected){
	this.ever_connected = true;
	get_Main().bootstrap++;
	}
	}else{
	
	connection_time = 0.01;
	
	}
	
	]]></Body>
				</Function>
