		<JavaClass>
			<Id>1380017911996</Id>
			<Name><![CDATA[PointEntry]]></Name>
			<Text><![CDATA[/**
 * PointEntry
 */	
public class PointEntry implements java.io.Serializable {

	ArrayList pointList;
    /**
     * Default constructor
     */
    public PointEntry(){
    	
    	this.pointList = new ArrayList<Double>();
    	
    }
    
 public PointEntry(int size){
    	
    	this.pointList = new ArrayList<Double>(size);
    	for (int i=0;i<size;i++)
    		this.pointList.add(0.0);
    	
    }
     
	public void AddValue (double value){
		
		this.pointList.add(value);
		
	}
	
	public double getSum (){
		
		double sum = 0.0;
		for (int i = 0; i < this.pointList.size(); i++)
			sum = sum + (Double)this.pointList.get(i);
		
		return sum;
		
	}
	
	public double getAvg (){
		
		double avg = 0.0;
		avg = getSum() / this.pointList.size();
		return avg;
		
	}
	
public double getValue (int index){
		
		double value = 0.0;
		value = (Double)this.pointList.get(index);
		return value;
		
	}

public boolean checkInteraction (){
	
		if((Double)this.pointList.get(3) != 0.0)
			return true;
		return false;
	
}

public double computeBalance (){
	
	double suportivecredits = (Double)this.pointList.get(0);
	double inconclusivecredits = (Double)this.pointList.get(1);
	double negativecredits = (Double)this.pointList.get(2);
	
	return (suportivecredits-inconclusivecredits-negativecredits);
}

public void setValue (int index, double value){
	
	this.pointList.set(index, value);
	
}

public boolean isEmpty (){
	
	return this.pointList.isEmpty();
	
}

public String PrintEntry () {
	
	String values = "";
	for (int i = 0; i < this.pointList.size(); i++){
		values = values + this.pointList.get(i);
		values = values + " - ";
	}
	
	return values;
	
}
	

}]]></Text>
